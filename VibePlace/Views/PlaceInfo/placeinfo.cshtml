@using VibePlace.Data.Models
@model PlaceToService


@section Style{
.address-product {
transition: all .3s;
}
.address-product:hover {
scale: 1.015;
background-color: #FEF3C7;
}
@@keyframes appear {
from {
opacity: 0;
transform: translateX(-80px);
/* scale: 0.5; */
}
to {
opacity: 1;
transform: translateX(0px);
/* scale: 1; */
}
}
@@keyframes appear-right {
from {
opacity: 0;
transform: translateX(80px);
/* scale: 0.5; */
}
to {
opacity: 1;
transform: translateX(0px);
/* scale: 1; */
}
}
@@keyframes appear-scale {
from {
opacity: 0;
scale: 0.5;
}
to {
opacity: 1;
scale: 1;
}
}
.animation-product {
animation: appear linear;
animation-timeline: view();
animation-range: entry 0% cover 30%;
}
.animation-product-right {
animation: appear-right linear;
animation-timeline: view();
animation-range: entry 0% cover 30%;
}
.animation-product-scale {
animation: appear-scale linear;
animation-timeline: view();
animation-range: entry 0% cover 30%;
}
}

