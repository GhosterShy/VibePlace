@using VibePlace.Data
@using VibePlace.Data.Models
@model PlacesCategoryModel




@{

    ViewData["Title"] = "Home Page";
}



<!-- Main Start -->
<main class="max-w-7xl mx-auto pb-6">
    <nav class="flex mt-3 mb-6 px-2 md:px-0" aria-label="Breadcrumb">
        <ol class="inline-flex items-center space-x-1 md:space-x-3">
            <li class="inline-flex items-center">
                <a href="javascript:;" class="inline-flex items-center text-base font-medium text-gray-900 hover:text-indigo-800">Home</a>
            </li>
            <li aria-current="page">
                <div class="flex items-center">
                    <svg class="mx-1 w-5 h-5" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M7.5 15L11.0858 11.4142C11.7525 10.7475 12.0858 10.4142 12.0858 10C12.0858 9.58579 11.7525 9.25245 11.0858 8.58579L7.5 5" stroke="#E5E7EB" stroke-width="1.6" stroke-linecap="round" stroke-linejoin="round" />
                    </svg>
                    <span class="ml-1 text-base font-medium text-indigo-600 md:ml-2 ">Тойхана</span>
                </div>
            </li>

        </ol>
    </nav>

    <!--FILTER -->
    <div class="filter-contents-home w-full flex gap-2 whitespace-nowrap overflow-auto mb-6 px-2 md:px-0" style="scrollbar-width: none; -ms-overflow-style: none;">

        <a href ="#">
        <div class="block">
            <input type="radio" name="radio-in-form" class="relative hidden checked:bg-no-repeat checked:bg-center checked:border-indigo-500 checked:bg-indigo-100" id="radio-in-form-00" checked>
            <label for="radio-in-form-00" class="h-full flex p-3 block w-full bg-white border border-gray-200 rounded-md text-sm pointer">
                <span class="border border-gray-300  rounded-full mr-2 w-4 h-4 mt-0.5"></span>
                <h5 class="text-sm text-gray-500 no-copy">All</h5>
            </label>
        </div>
        </a>


        @foreach (Category category in Model.Categories)
        {
            <div class="block">
                <input type="radio" name="radio-in-form" class="relative hidden checked:bg-no-repeat checked:bg-center checked:border-indigo-500 checked:bg-indigo-100" id="radio-in-form-@category.id" checked>
                <label for="radio-in-form-@category.id" class="h-full flex p-3 block w-full bg-white border border-gray-200 rounded-md text-sm pointer">
                    <span class="border border-gray-300  rounded-full mr-2 w-4 h-4 mt-0.5"></span>
                    <a hx-get="/Home/FilterPlaces/@category.id"
                       hx-target="#places-container"
                       hx-trigger="change">
                    <h5 class="text-sm text-gray-500 no-copy">@category.categoryName</h5>
                    </a>
                </label>
            </div>
         

        }
    </div>

    <!--FILTER End-->
  
   
    <!--Carts-->
    <div class="w-full relative flex items-center justify-center h-auto mb-3">
        <div id="places-container" class="grid grid-cols-1 px-2 md:px-0 md:grid-cols-4 gap-5" id="grid-container-home">


            @* Item_Cats *@

