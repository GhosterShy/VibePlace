            {
                <div class="rounded-xl bg-white shadow-lg shadow-gray-300 overflow-hidden relative grid-item-home">
                    <a href="@Url.Action("placeinfo","PlaceInfo", new { id = place.Id })" class="absolute opacity-0 h-full w-full rounded-xl"></a>
                    <img src="@Url.Content(place.Image)" alt="image">
